{
  "name": "One Darker Vivid Italic",
  "author": "CJ Ratliff",
  "authorUrl": "https://github.com/cjratliff",
  "description": "A dark gray variant of the infamous Atom's iconic One Dark theme, and one of the most downloaded themes for VS Code.",
  "version": "1.0.0",
  "dark": true,
  "colors": {
    "backgroundDark": "#1f1f1f",
    "backgroundDarker": "#181818",
    "accentText": "#00d9ff",
    "whiteText": "#ffffff"
  },
  "ui": {
    "*": {
      "background": "backgroundDarker"
    },
    "ToolBar.background": "backgroundDarker",
    "SidePanel.background": "backgroundDarker",
    "Panel.background": "backgroundDarker",
    "Button.background": "backgroundDark",
    "Editor.background": "backgroundDark"
  },
  "Editor.background": "backgroundDark",
  "Button.background": "backgroundDark",
  "Button.default.foreground": "whiteText",
  "Panel.foreground": "whiteText",
  "Panel.foreground.active": "accentText",
  "Panel.foreground.hover": "accentText",
  "Panel.hoverForeground": "backgroundDark"
}